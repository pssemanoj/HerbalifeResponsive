@using MyHerbalife3.Shared.UI.Extensions
@model MyHerbalife3.Ordering.ViewModel.Model.InvoiceModel

@{
    Layout = null;
    var culture = System.Globalization.CultureInfo.CurrentCulture.ToString();
    var print = 1;
    var countryCode = culture.Substring(3, 2);
    if (culture == "en-US" || culture == "es-US")
    {
        print = 2;
    }
    bool hide = false;
    bool HideControl = false;
    if(Model.ReceiptChannel== "ClubSaleReceipt" || Model.ReceiptChannel == "Club Visit/Sale")
    {
        hide = true;
    }
    else
    {
        hide = false;
    }
    if (countryCode == "CA" || countryCode == "TT" || countryCode == "JM")
    {
        HideControl = true;
    }
    else
    {
        HideControl = false;
    }

}

<!DOCTYPE html>

<html>
    <head>
        <title>Invoice Print</title>
    </head>
    <body >
        <div class="container">
            <style type="text/css">
                body {
                    font-family: "Proxima Nova",sans-serif;
                    font-size: 14PX;
                    color: #333;
                }

                .container {
                    width: 850px;
                    margin: auto;
                }

                h1, h2 {
                    font-size: 1.5em;
                }

                h1 {
                    float: right;
                }

                h2 {
                    float: left;
                }


                .bold {
                    font-weight: bold;
                }

                .center {
                    text-align: center;
                }

                .left {
                    text-align: left;
                }

                .right {
                    text-align: right;
                }

                .row {
                    display: inline-block;
                    margin: 0;
                    width: 100%;
                }

                .header {
                    width: 100%;
                    margin: 10px 0;
                }

                    .header div {
                        box-sizing: border-box;
                        float: left;
                        position: relative;
                    }

                    .header div:first-child {
                        width: 145px;
                    }

                .subHeader, .shippingAddress, .totals {
                    display: inline-block;
                    height: auto;
                }
                
                .paymentAddress, .invoiceOrder, .shipment {
                    box-sizing: border-box;
                    clear: both;
                    overflow: hidden;
                    display: inline-block;
                    font-size: 14px;
                    text-rendering: optimizeLegibility;
                    width: 300px;
                    height: auto;
                }

                    .paymentAddress span, .shipment span {
                        display: block;
                        padding-bottom: 5px;
                    }

                    .invoiceOrder span {
                        padding-bottom: 10px;
                    }

                        .invoiceOrder span:first-child {
                            width: 150px;
                        }

                .clear {
                    clear: both;
                }

                .go-left {
                    float: left;
                }

                .go-right {
                    float: right;
                }

                .grayBar {
                    background: #f3f3f3 none repeat scroll 0 0;
                    border: medium none;
                    height: 10px;
                    margin: 10px auto;
                    padding: 0 35px;
                }

                .shipment {
                    /*margin-left: 15px;*/
                }

                .shippingAddress .shipment,
                .shippingAddress .notes {
                    margin-top: 15px;
                    vertical-align: top;
                }

                .notes {
                    box-sizing: border-box;
                    font-size: 16px;
                    height: 100px;
                    width: 525px;
                }

                #tblInvLines {
                    margin: 10px auto;
                    width: 100%;
                }

                    #tblInvLines tr {
                        height: 40px;
                    }

                    #tblInvLines th {
                        border-bottom: 1px solid #cccccc;
                        color: #000;
                        line-height: 1.3em;
                        overflow: hidden;
                        padding: 0.5em 0.6em 0.4em;
                        text-overflow: ellipsis;
                }

                    #tblInvLines td {
                        overflow: hidden;
                        text-overflow: ellipsis;
                }

                .even {
                    background-color: #FFF;
                }

                .odd {
                    background-color: #f3f3f3;
                }

                .totals table {
                    /*margin-right: 35px;*/
                    width: 400px;
                }

                    .totals table tr {
                        height: 30px;
                    }

                .total-due {
                    font-size: 18px;
                }

                .logo {
            background: url(data:image/png;base64,) no-repeat;
                height:63px;
                width:220px!important;
        }

            </style>
           

           
            @for (var i = 0; i < print; i++)
            {
                var h = 1;
                if (i == 1)
                {
                    <div style="height: 40px;"></div>
                    <div style="page-break-before: always;"></div>
                    <div style="height: 40px;"></div>

                }
                <div class="header">
                    @*
                    <h2>@this.Localize("thankyou")</h2>*@
                    <div class="header row" style="width: 100%">
                        <div class="logo">
                        </div>
                        <div style="display: inline;">
                            <h1>@this.Localize("invoice")</h1>
                        </div>
                    </div>
                </div>
                <div class="center">
                 @if (HideControl)
                     {
                    <span>@this.Localize("thanksmessage1")</span>
                    <br />
                    <span>@this.Localize("thanksmessage2")</span>
                     }
                </div>
                <br />
                <br />
                <div class="subHeader clear">
                    <div class="paymentAddress go-left">
                        <span>@Model.MemberFirstName @Model.MemberLastName</span>
                        <span>@Model.MemberEmailAddress</span>
                        @if (HideControl)
                        {
                        <span>@Model.MemberPhoneNumber</span>
                        }
                        @if (Model.MemberAddress != null && !HideControl)
                        {
                            @*<strong>@this.Localize("pleaseSendPayment"):</strong>*@
                           
                            <span>@Model.MemberAddress.Address1</span>
                            @*<span>@Model.MemberAddress.Address2</span>*@
                            <span>@Model.MemberAddress.City</span>
                            <span>@Model.MemberAddress.State @Model.MemberAddress.PostalCode</span>
                           
                        }
                    </div>
                    <div class="invoiceOrder go-right">
                        <div class="clear">
                            <span class="bold go-left right">@this.Localize("invoiceId"):</span>
                            <span class="go-right">@Model.DisplayMemberInvoiceNumber</span>
                        </div>
                        <div class="clear">
                            <span class="bold go-left right">@this.Localize("invoiceDate"):</span>
                            <span class="go-right">@Model.InvoiceDate.ToShortDateString()</span>
                        </div>
                        <div>
                            @if (!HideControl)
                            {
                            <div class="clear">
                                <span class="bold go-left right">@this.Localize("status"):</span>
                                <span class="go-right" style="position:relative">@Model.DisplayInvoiceStatus</span>
                            </div>
                            }
                            else
                            {
                                <div class="clear">
                                    <span class="bold go-left right">@this.Localize("ReceiptChannel"):</span>
                                    <span class="go-right" style="position:relative">@Model.DisplayReceiptChannel</span>
                                </div>
                                <div class="clear">
                                    <span class="bold go-left right">@this.Localize("paymentType"):</span>
                                    <span class="go-right" style="position:relative">@Model.DisplayPaymentType</span>
                                </div>
                            }

                        </div>
                    @if (@Model.IsDisplayVat)
                    {
                            <div class="clear">
                                <span class="bold go-left right">@this.Localize("vat"):</span>
                                <span class="go-right" style="position:relative">@Model.Vat</span>
                            </div>
                    }
                    </div>
                </div>
                <div class="grayBar"></div>
                <div class="shippingAddress clear" >
                    <div class="bold" >@this.Localize("shipTo"):</div>
                    <div class="shipment go-left">
                        <span>@Model.FirstName @Model.LastName</span>
                        <span>@Model.Address.Address1 @Model.Address.Address2</span>
                        <span>@Model.Address.City @Model.Address.State @Model.Address.PostalCode</span>
                        <span>@Model.Email</span>
                        <span>@Model.Phone</span>
                    </div>
                    <div class="notes go-right">
                        @if (!HideControl)
                        {
                        <span class="bold">@this.Localize("notes"):</span>
                        <p>@Model.Notes</p>
                        }
                    </div>
                </div>
                <div class="grayBar"></div>
                <div>
                 @if (HideControl && hide) {
                   <table class="go-right">
                       <tr style="font-weight:bold">
                           <td class="left">@this.Localize("Description"):</td>
                           <td class="right">@Model.ClubInvoice.ClubRecieptProductName</td>
                       </tr>
                       <tr style="font-weight:bold">
                           <td class="left">@this.Localize("Quantity"):</td>
                           <td class="right">@Model.ClubInvoice.ClubRecieptQuantity</td>
                       </tr>
                       <tr style="font-weight:bold">
                           <td class="left"><h3>@this.Localize("TotalDue"):</h3></td>
                           <td class="right"><h3>@this.Localize("currencySign")@Model.InvoicePrice.DisplayTotalYourPrice</h3></td>
                       </tr>
                   </table>
                   <div class="clear"></div>
               }
               else
               {
                <table id="tblInvLines" cellpadding="0" cellspacing="0">
                    <thead>
                        <tr>
                            <th>@this.Localize("sku")</th>
                            <th class="left">@this.Localize("productName")</th>
                            <th>@this.Localize("qty")</th>
                            <th>@this.Localize("retailPrice")</th>

                            @if (@Model.Type == "Distributor")
                            {
                                <th>@this.Localize("earnBase")</th>
                            }
                            <th>@this.Localize("totalPrice")</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in @Model.InvoiceLines)
                        {
                            var alterRow = "odd";
                            if (h % 2 == 0)
                            {
                                alterRow = "even";
                            }

                            <tr>
                                <td class="@alterRow center">@item.Sku</td>
                                <td class="@alterRow">@Html.Raw(item.ProductName)</td>
                                <td class="@alterRow center">@item.Quantity</td>
                                <td class="@alterRow right">@item.DisplayRetailPrice</td>
                                @if (@Model.Type == "Distributor")
                                {
                                    <td class="@alterRow right">@item.EarnBase</td>
                                }
                                <td class="@alterRow right">@item.DisplayTotalRetailPrice</td>
                            </tr>
                            h++;
                        }
                    </tbody>
                </table>

                <div class="totals clear">
                    <table class="go-right" cellpadding="0" cellspacing="0">
                        <tbody>
                            <tr>
                                <td class="right">@this.Localize("subtotal"):</td>
                                <td class="right">@Model.InvoicePrice.DisplaySubtotal</td>
                            </tr>
                             @if (Model.InvoicePrice.DiscountPercentage > 0 || Model.InvoicePrice.DiscountAmount > 0)
                            { 
                            <tr>
                               
                                <td class="right">@this.Localize("discount"):</td>
                                <td class="right">@Model.InvoicePrice.DisplayDiscountedAmount</td>
                                
                            </tr>
                            }
                            <tr>

                                @if (Model.IsDisplayFreeShipping)
                                {
                                    <td class="right">@this.Localize("freeShippingAndHandling"):</td>
                                }
                                else
                                {
                                    <td class="right">@this.Localize("shippingAndHandling"):</td>
                                }

                                <td class="right">@Model.InvoicePrice.DisplayShipping</td>
                            </tr>

                            <tr>
                                <td class="right">@this.Localize("tax"):</td>
                                <td class="right">@Model.InvoicePrice.DisplayCalculatedTax</td>
                            </tr>

                            <tr class="total-due bold">
                                <td class="right">@this.Localize("yourTotalDue"):</td>
                                <td class="right">@Model.InvoicePrice.DisplayTotalDue</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
               }

             </div>
                <div class="disclaimer">
                    @if (!HideControl)
                {
                    <p>
                        @this.Localize("iunderStandText")
                    </p>
                    <strong>@this.Localize("importantNotice")</strong>
                    <p></p>
                
                    <p>@this.Localize("buyerText")</p>}
                </div>

                @Html.Partial("Footer/Disclaimers/Disclaimer_" + countryCode)
                }
                </div >
    </body>
</html>